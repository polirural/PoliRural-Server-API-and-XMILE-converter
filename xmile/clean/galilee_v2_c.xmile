				<connect2 to="RURAL_RETENTION_CAPACITY.community_and_public_services_accessibility" from="RURAL_ATTRACTIVENESS.community_and_public_services_accessibility"/>
				<connect to="RURAL_RETENTION_CAPACITY.ELDERLY_POPULATION" from="POPULATION.ELDERLY_POPULATION"/>
				<connect2 to="RURAL_RETENTION_CAPACITY.ELDERLY_POPULATION" from="POPULATION.ELDERLY_POPULATION"/>
				<connect to="RURAL_RETENTION_CAPACITY.health_centers_accessibility" from="QUALITY_OF_LIFE.health_centers_accessibility"/>
