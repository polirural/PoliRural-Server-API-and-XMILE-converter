				<connect2 to="RURAL_ATTRACTIVENESS.ELDERLY_POPULATION" from="POPULATION.ELDERLY_POPULATION"/>
				<connect to="RURAL_ATTRACTIVENESS.employment_gap_1" from="EMPLOYMENT.employment_gap"/>
				<connect2 to="RURAL_ATTRACTIVENESS.employment_gap_1" from="EMPLOYMENT.employment_gap"/>
				<connect to="RURAL_ATTRACTIVENESS.health_centers_accessibility" from="QUALITY_OF_LIFE.health_centers_accessibility"/>
				<connect2 to="RURAL_ATTRACTIVENESS.health_centers_accessibility" from="QUALITY_OF_LIFE.health_centers_accessibility"/>
				<connect to="RURAL_ATTRACTIVENESS.natural_capital_perception" from="NATURAL_CAPITAL.natural_capital_perception"/>
				<connect2 to="RURAL_ATTRACTIVENESS.natural_capital_perception" from="NATURAL_CAPITAL.natural_capital_perception"/>
				<connect to="RURAL_ATTRACTIVENESS.total_population_in_relation_with_initial_population" from="POPULATION.total_population_in_relation_to_initial_population"/>
				<connect2 to="RURAL_ATTRACTIVENESS.total_population_in_relation_with_initial_population" from="POPULATION.total_population_in_relation_to_initial_population"/>
